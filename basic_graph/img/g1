digraph {
	graph [bgcolor="#333333" fontcolor=white fontsize=16 label="A hella fancy grapherino" rankdir=TB]
	node [color=white fillcolor="#006699" fontcolor=white fontname=Helvetica shape=ellipse style=filled]
	edge [arrowhead=open color=white fontcolor=white fontname=Courier fontsize=12 style=dashed]
		START [fillcolor="#21BA65"]
		STOP [fillcolor="#B44B6E"]
		B [label="how many?" shape=hexagon]
		C [label=2]
		D [label=40]
		E [label=58]
			START -> B
			B -> C
			B -> D
			B -> E
			C -> STOP
			D -> STOP
			E -> STOP
}